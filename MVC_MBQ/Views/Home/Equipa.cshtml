@{
    ViewData["Title"] = "Equipa";
}
<section id="equipa" class="py-5 bg-light text-center">
    <div class="container">
        <div class="row align-items-center justify-content-center">
            <div class="col text-center">
                <h1 class="display-4 text-uppercase text-dark">
                    <strong>Equipa</strong>
                </h1>
                <div class="title-underline bg-danger"></div>
                <p class="text-muted">Equipa WarmBabies.</p>
            </div>
        </div>
        <!--end of title-->
        <div class="row">
            <div class="col-md-6 col-lg-4 my-3">
                <div class="card">
                    <img src="~/imagens/Foto Ana.jpg" alt="Fotografia Ana" class="card-img-top">
                    <div class="card-body">
                        <h5 class="card-title text-capitalize">Ana Domingues</h5>
                        <p class="card-text">Com olho no Futuro Digital. A apostar na formação - UPSkill </p>
                        <a class="btn btn-outline-dark text-uppercase" asp-controller="Home" asp-action="AnaDomingues">Aceder a Pagina</a>
                    </div>
                    <div class="card-footer bg-secondary d-flex justify-content-around">
                    </div>
                </div>
            </div>
            <!--fim do card-->
            <div class="col-md-6 col-lg-4 my-3">
                <div class="card">
                    <img src="~/imagens/catia_sa.png" alt="Fotografia Cátia" class="card-img-top">
                    <div class="card-body">
                        <h5 class="card-title text-capitalize">Cátia Sá</h5>
                        <p class="card-text">Com elevada capacidade de aprendizagem. A frequentar o <a href="https://upskill.pt/">UpSkill</a></p>
                        <a href=" https://catiamsa.github.io/PaginaCatiaSa/" target="_blank" class="btn btn-outline-dark text-uppercase">Aceder a Página</a>
                    </div>
                    <div class="card-footer bg-secondary d-flex justify-content-around">
                    </div>
                </div>
            </div>
            <!--fim do card-->
            <div class="col-md-6 col-lg-4 my-3">
                <div class="card">
                    <img src="~/imagens/Carlos Brito.png" alt="Fotografia Carlos" class="card-img-top">
                    <div class="card-body">
                        <h5 class="card-title text-capitalize">Carlos Brito</h5>
                        <p class="card-text">Futuro programador a aprender .NET no programa UPSkill.</p>
                        <a class="btn btn-outline-dark text-uppercase" asp-controller="Home" asp-action="CarlosBrito">Aceder a Pagina</a>
                    </div>
                    <div class="card-footer bg-secondary d-flex justify-content-around">
                    </div>
                </div>
            </div>
            <!--fim do card-->
            <div class="col-md-6 col-lg-4 my-3">
                <div class="card">
                    <img src="~/imagens/fotoRui.jpg" alt="Fotografia Rui" class="card-img-top">
                    <div class="card-body">
                        <h5 class="card-title text-capitalize">Rui Rodrigues</h5>
                        <p class="card-text">Futuro Fullstacker! De momento a aprender c#, Azure e .net.</p>
                        <a href="https://ruirodrigues.netlify.app/" class="btn btn-outline-dark text-uppercase" target="_blank">Aceder a Pagina</a>
                    </div>
                    <div class="card-footer bg-secondary d-flex justify-content-around">
                    </div>
                </div>
            </div>
            <!--fim do card-->
            <div class="col-md-6 col-lg-4 my-3">
                <div class="card">
                    <img src="~/imagens/imgJoao.jpg" alt="Fotografia João" class="card-img-top">
                    <div class="card-body">
                        <h5 class="card-title text-capitalize">João Silva</h5>
                        <p class="card-text">Curioso por natureza e pela Natureza, agora a fazer upgrade para o futuro digital.</p>
                        <a class="btn btn-outline-dark text-uppercase" href="https://jmsilva90.github.io/">Aceder a Pagina</a>
                    </div>
                    <div class="card-footer bg-secondary d-flex justify-content-around">
                    </div>
                </div>
            </div>
            <!--fim do card-->
            <div class="col-md-6 col-lg-4 my-3">
                <div class="card">
                    <img src="~/imagens/fotografiaPAPIEquipa.png" alt="Fotografia Papi" class="card-img-top">
                    <div class="card-body">
                        <h5 class="card-title text-capitalize">Pápi</h5>
                        <p class="card-text">O membro mais importante da equipa de desenvolvimento.</p>
                        <a asp-controller="Home" asp-action="Index" class="btn btn-outline-dark text-uppercase">Aceder a Pagina</a>
                    </div>
                    <div class="card-footer bg-secondary d-flex justify-content-around">
                    </div>
                </div>
            </div>
            <!--fim do card-->
        </div>
    </div>
</section>